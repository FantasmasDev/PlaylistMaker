    <style name="SettingsScreenActivity">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:background">?attr/prime_color</item>
        <item name="android:orientation">vertical</item>
    </style>

    <style name="ScreenHeaderLayout">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:orientation">horizontal</item>
        <item name="android:padding">4dp</item>
    </style>

    <style name="ScreenHeaderButton">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_gravity">start</item>
        <item name="android:background">?attr/prime_color</item>
        <item name="android:importantForAccessibility">no</item>
        <item name="android:padding">12dp</item>
    </style>


    <style name="ScreenOption">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:orientation">horizontal</item>
        <item name="android:paddingStart">@dimen/padding_biggest_16dp</item>
        <item name="android:paddingTop">@dimen/padding_biggest_16dp</item>
        <item name="android:paddingEnd">@dimen/padding_big_12dp</item>
        <item name="android:paddingBottom">@dimen/padding_biggest_16dp</item>
    </style>

    <style name="ScreenOptionText">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:layout_gravity">start</item>
        <item name="android:layout_weight">1</item>
        <item name="android:textColor">?attr/on_prime_color</item>
        <item name="android:textSize">@dimen/text_size_medium_16sp</item>
        <item name="android:fontFamily">@font/ys_display_medium</item>
    </style>

    <style name="ScreenOptionImage">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_gravity">end</item>
        <item name="android:background">?attr/prime_color</item>
    </style>

    <style name="SearchBarLayout">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:maxHeight">36dp</item>
        <item name="android:background">@drawable/corner_rectangle</item>
        <item name="android:layout_marginHorizontal">@dimen/margin_biggest_16dp</item>
        <item name="android:layout_marginTop">@dimen/margin_medium_8dp</item>
    </style>

    <style name="SearchBar">
        <item name="android:inputType">text</item>
        <item name="android:drawablePadding">10dp</item>
        <item name="android:paddingStart">@dimen/padding_big_12dp</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">36dp</item>
        <item name="android:minHeight">36dp</item>
        <item name="android:maxHeight">36dp</item>
        <item name="android:maxLines">1</item>
        <item name="android:fontFamily">@font/ys_display_regular</item>
        <item name="android:backgroundTint">?attr/shape_back_ground</item>
        <item name="android:textSize">@dimen/text_size_medium_16sp</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:textColorHint">?attr/on_shape_back_ground</item>
        <item name="android:drawableTint">?attr/on_shape_back_ground</item>
    </style>

    <style name="ClearButton">
        <item name="android:layout_width">36dp</item>
        <item name="android:layout_height">36dp</item>
        <item name="android:backgroundTint">?attr/shape_back_ground</item>
        <item name="android:layout_gravity">end</item>
    </style>

    <style name="TrackPlate">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:orientation">horizontal</item>
        <item name="android:paddingVertical">@dimen/padding_medium_8dp</item>
        <item name="android:paddingStart">@dimen/padding_biggest_16dp</item>
        <item name="android:paddingEnd">@dimen/padding_big_12dp</item>
        <item name="android:background">?attr/prime_color</item>
    </style>

    <style name="TrackAlbum">
        <item name="android:layout_width">45dp</item>
        <item name="android:layout_height">45dp</item>
        <item name="android:layout_marginEnd">@dimen/margin_medium_8dp</item>
    </style>

    <style name="TrackTextList">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_gravity">center</item>
        <item name="android:layout_marginEnd">@dimen/margin_medium_8dp</item>
        <item name="android:layout_weight">1</item>
        <item name="android:orientation">vertical</item>
    </style>

    <style name="TrackNameText">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:fontFamily">@font/ys_display_regular</item>
        <item name="android:textColor">?attr/on_prime_color</item>
        <item name="android:maxHeight">19dp</item>
        <item name="android:textSize">@dimen/text_size_medium_16sp</item>
    </style>

    <style name="TrackArtistName">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:fontFamily">@font/ys_display_regular</item>
        <item name="android:textColor">?attr/search_text_main_color</item>
        <item name="android:textSize">11sp</item>
        <item name="android:maxHeight">13dp</item>
    </style>

    <style name="TrackSeparator">
        <item name="android:layout_width">13dp</item>
        <item name="android:layout_height">13dp</item>
    </style>

    <style name="TrackTime">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:fontFamily">@font/ys_display_regular</item>
        <item name="android:textColor">?attr/search_text_main_color</item>
        <item name="android:textSize">11sp</item>
        <item name="android:maxHeight">13dp</item>
    </style>

    <style name="TrackActionButton">
        <item name="android:layout_width">24dp</item>
        <item name="android:layout_height">24dp</item>
        <item name="android:layout_gravity">center</item>
    </style>

    <style name="placeholder_button">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_gravity">center</item>
        <item name="android:layout_marginTop">24dp</item>
        <item name="android:backgroundTint">?attr/on_prime_color</item>
        <item name="android:textColor">?attr/prime_color</item>
        <item name="android:textSize">14sp</item>
        <item name="android:visibility">gone</item>
    </style>

    <style name="placeholder_text">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_gravity">center</item>
        <item name="android:layout_marginTop">@dimen/margin_biggest_16dp</item>
        <item name="android:gravity">center</item>
        <item name="android:textColor">?attr/on_prime_color</item>
        <item name="android:textSize">19sp</item>
    </style>

    <style name="placeholder_image">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_gravity">center</item>
        <item name="android:importantForAccessibility">no</item>
        <item name="android:minWidth">30dp</item>
        <item name="android:minHeight">30dp</item>
    </style>

    <style name="library_fragment_button">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginTop">24dp</item>
        <item name="android:backgroundTint">?attr/on_prime_color</item>
        <item name="android:fontFamily">@font/ys_display_medium</item>
        <item name="android:textColor">?attr/prime_color</item>
        <item name="android:textSize">16sp</item>
    </style>

    <style name="library_place_holder_image">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginTop">106dp</item>
        <item name="android:importantForAccessibility">no</item>
    </style>

    <style name="library_placeholder_text">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginTop">16dp</item>
        <item name="android:fontFamily">@font/ys_display_medium</item>
        <item name="android:textColor">?attr/on_prime_color</item>
        <item name="android:textSize">19sp</item>
    </style>

    <style name="library_header_text">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginTop">16dp</item>
        <item name="android:layout_marginStart">16dp</item>
        <item name="android:fontFamily">@font/ys_display_medium</item>
        <item name="android:gravity">start|center</item>
        <item name="android:textColor">?attr/on_prime_color</item>
        <item name="android:textSize">22sp</item>
    </style>

    <style name="library_header_back_button">
        <item name="android:layout_width">24dp</item>
        <item name="android:layout_height">24dp</item>
        <item name="android:layout_marginStart">16dp</item>
        <item name="android:layout_marginTop">16dp</item>
        <item name="android:background">@null</item>
        <item name="android:importantForAccessibility">no</item>
    </style>

    <style name="ScreenHeaderText">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">?attr/on_prime_color</item>
        <item name="android:textSize">@dimen/text_size_big_22sp</item>
        <item name="android:fontFamily">@font/ys_display_medium</item>
        <item name="android:layout_marginStart">@dimen/margin_biggest_16dp</item>
        <item name="android:layout_marginTop">@dimen/margin_biggest_16dp</item>
    </style>

</resources>